<Project Sdk="Microsoft.NET.Sdk.Razor">
  <PropertyGroup Label="Application">
    <TargetFramework>netstandard2.0</TargetFramework>
    <Product>DannyBoyNg UserAgentParser NetStandard Extensions</Product>
    <AssemblyVersion>2019.0911.0652.0</AssemblyVersion>
    <FileVersion>2019.0911.0652.0</FileVersion>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
  </PropertyGroup>

  <ItemGroup Label="Includes">
    <EmbeddedResource Include="Pages\**\*" />
  </ItemGroup>

  <ItemGroup Label="PackageReferences">
    <PackageReference Include="DannyBoyNg.UserAgentParser" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc" />
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles" />
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" />
  </ItemGroup>

</Project>