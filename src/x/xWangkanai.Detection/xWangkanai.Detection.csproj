<Project Sdk="Microsoft.NET.Sdk.Razor">
  <PropertyGroup Label="Application">
    <TargetFramework>netstandard2.0</TargetFramework>
    <Product>Wangkanai .Detection NetStandard Extensions</Product>
    <VersionPart1>2</VersionPart1>
    <VersionPart2>0</VersionPart2>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
  </PropertyGroup>
  <ItemGroup Label="Includes">
    <EmbeddedResource Include="Pages\**\*" />
  </ItemGroup>
  <ItemGroup Label="PackageReferences">
    <PackageReference Include="Microsoft.AspNetCore.Mvc" />
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles" />
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" />
    <PackageReference Include="Wangkanai.Detection" />
  </ItemGroup>
</Project>