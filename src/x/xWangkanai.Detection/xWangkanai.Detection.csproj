<Project Sdk="Microsoft.NET.Sdk.Razor">
  <PropertyGroup Label="Application">
    <TargetFramework>netstandard2.0</TargetFramework>
    <Product>Wangkanai .Detection NetStandard Extensions</Product>
    <VersionPart1>2</VersionPart1>
    <VersionPart2>0</VersionPart2>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
  </PropertyGroup>
  <ItemGroup Label="Includes">
    <EmbeddedResource Include="Pages\**\*" />
  </ItemGroup>
  <ItemGroup Label="PackageReferences">
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.2.0" />
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="8.0.8" />
    <PackageReference Include="Wangkanai.Detection" Version="2.0.0" />
  </ItemGroup>
</Project>